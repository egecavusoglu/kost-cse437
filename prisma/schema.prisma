// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

// Data Models

// User Model
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  hashedPassword String
  firstName String
  lastName String
}